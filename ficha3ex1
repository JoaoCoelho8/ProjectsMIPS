		.data
MSG:		.asciiz "o valor final é: "
		.text
	
Main:		li $s0, 0
		li $s1, 0
Loop:		beq $s0,10,End
		move $a0,$s0
		addi $a1,$a0,1
		jal Sumsquare
		add $s1,$s1,$v0
		addi $s0,$s0,1
		j Loop
	
End:		li $v0, 4
		la $a0, MSG
		syscall
		li $v0, 1
		move $a0, $s1
		syscall
		li $v0, 10
		syscall
		
Sumsquare:	mul $t0, $a0, $a0
		mul $t1, $a1, $a1
		add $v0, $t0, $t1
		jr $ra
		