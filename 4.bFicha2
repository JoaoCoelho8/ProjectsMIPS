	.data
vec:	.word 1,3,5,7,9,11,13,15
MSG:	.asciiz "O valor final é "
	.text
	
	li $s0, 0
	li $s1, 0
	la $t0, vec
	
L1:	beq $s0, 8, End
	lw $t1, 0($t0)
	add $s1, $s1, $t1
	addi $t0, $t0, 4
	addi $s0, $s0, 1
	j L1

End:	li $v0, 4
	la $a0, MSG
	syscall
	li $v0, 1
	move $a0, $s1
	syscall
	li $v0, 10
	syscall
